{
  "name": "eslint-config-airbnb",
  "version": "19.0.4",
  "description": "Airbnb's ESLint config, following our styleguide",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./base": "./base.js",
    "./hooks": "./hooks.js",
    "./legacy": "./legacy.js",
